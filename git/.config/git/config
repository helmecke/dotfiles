[push]
	default = current
	followTags = true
[alias]
	cc = commit
	ca = commit --amend
	ce = commit --amend --no-edit
	co = checkout
	wt = worktree
	sync = !git fetch --prune
	cleanup = !bash ~/.local/bin/git-cleanup
[ghq]
	root = ~/Git
[init]
	defaultBranch = master
[core]
	excludesfile = ~/.config/git/ignore
